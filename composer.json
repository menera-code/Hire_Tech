{
    "name": "menera-code/hire-tech",
    "description": "HireTech Application with LavaLust Framework",
    "type": "project",
    "require": {
        "php": "^8.0",
        "ext-pdo": "*",
        "ext-mysqli": "*",
        "ext-gd": "*",
        "ext-mbstring": "*",
        "ext-curl": "*",
        "ext-zip": "*",
        "google/apiclient": "^2.15",
        "facebook/php-business-sdk": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "config/app.php"
        ]
    },
    "config": {
        "optimize-autoloader": true
    }
}
